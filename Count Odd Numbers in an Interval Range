class Solution {
public:
    int countOdds(int low, int high) {
       int count=0;
    for(int i=low;i<=high;i++)
        if(i%2==1) count++;
        return count;
 
    }
};

###Optimal Solution
class Solution {
public:
    int countOdds(int low, int high) {
        return (high + 1) / 2 - low / 2;      
    }
};
